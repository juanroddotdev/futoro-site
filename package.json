{
  "name": "FutoroDigital",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:strict": "vue-tsc && vite build",
    "preview": "vite preview",
    "deploy": "sh deploy.sh",
    "type-check": "vue-tsc --noEmit",
    "type-check:strict": "cross-env TS_NODE_COMPILER_OPTIONS='{\"strict\":true,\"noUnusedLocals\":true,\"noUnusedParameters\":true}' vue-tsc --noEmit",
    "build-no-types": "vite build",
    "analyze": "node scripts/run-all-analyses.js",
    "analyze:deps": "node scripts/analyze.js deps",
    "analyze:deps:circular": "node scripts/analyze.js deps:circular",
    "analyze:deps:graph": "node scripts/analyze.js deps:graph",
    "analyze:deps:violations": "node scripts/analyze.js deps:violations",
    "analyze:css": "node scripts/analyze.js css",
    "analyze:scss": "node scripts/analyze.js scss",
    "analyze:components": "node scripts/analyze.js components",
    "analyze:report": "node scripts/analyze.js report",
    "analyze:css-report": "npm run analyze:css && npm run analyze:components && npm run analyze:scss && npm run analyze:report"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.15.0",
    "@vueform/multiselect": "^2.6.11",
    "@vueuse/core": "^10.11.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "glob": "^11.0.1",
    "gsap": "^3.12.5",
    "lucide-vue-next": "^0.503.0",
    "reka-ui": "^2.2.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.15.0",
    "vivus": "^0.4.6",
    "vue": "^3.4.38",
    "vue-router": "^4.2.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/vivus": "^0.4.7",
    "@vitejs/plugin-vue": "^5.1.3",
    "autoprefixer": "^10.4.17",
    "dependency-cruiser": "^16.10.0",
    "postcss": "^8.4.35",
    "postcss-nesting": "^13.0.1",
    "purgecss": "^5.0.0",
    "sass": "^1.85.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "vue-tsc": "^2.1.4"
  }
}
